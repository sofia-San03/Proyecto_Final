{
  "env_name": "dev_local",
  "source_db": {
    "dbms": "postgres",
    "host": "localhost",
    "port": 5432,
    "user": "postgres",
    "password_env": "SRC_DB_PASS",
    "database": "app_prod"
  },
  "dest_db": {
    "dbms": "postgres",
    "host": "localhost",
    "port": 5432,
    "user": "postgres",
    "password_env": "DST_DB_PASS",
    "database": "app_qa"
  },
  "tables": [
    {
      "name": "customers",
      "watermark_column": "updated_at",
      "filter": null,
      "batch_size": 1000
    },
    {
      "name": "orders",
      "watermark_column": "updated_at",
      "filter": null,
      "batch_size": 1000
    },
    {
      "name": "order_items",
      "watermark_column": "updated_at",
      "filter": null,
      "batch_size": 1000
    }
  ],
  "masking_rules": {
    "customers": {
      "customer_id": "tokenize",
      "email": "deterministic_hash",
      "phone": "preserve_format",
      "full_name": "redaction"
    },
    "orders": {
      "customer_id": "tokenize",
      "card_number": "redaction"
    }
  },
  "token_vault_table": "token_vault",
  "audit_table": "execution_audit",
  "retry": {
    "max_attempts": 3,
    "backoff_seconds": 5
  },
  "mode": "delta",
  "dry_run": false
}
